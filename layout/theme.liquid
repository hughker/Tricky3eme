<head>
{% include 'meta' %}
<!-- For less capable mobile browsers
<link rel="stylesheet" media="handheld" href="handheld.css?v=1">  -->

<!-- For all browsers -->
<link rel="stylesheet" media="screen" href="{{ 'mangled.css?v=1' | asset_url }}">

<link rel="stylesheet" media="print" href="{{ 'print.css?v=1' | asset_url }}">
<!-- For progressively larger displays -->
<link rel="stylesheet" media="only screen and (min-width: 480px)" href="{{ '480.css?v=1' | asset_url }}">
<link rel="stylesheet" media="only screen and (min-width: 768px)" href="{{ '768.css?v=1'| asset_url }}">
<link rel="stylesheet" media="only screen and (min-width: 992px)" href="{{ '992.css?v=1'| asset_url }}">
<link rel="stylesheet" media="only screen and (min-width: 1382px)" href="{{ 1382.css?v=1' | asset_url }}">
<!-- For Retina displays -->
<link rel="stylesheet" media="only screen and (-webkit-min-device-pixel-ratio: 2), only screen and (min-device-pixel-ratio: 2)" href="{{ '2x.css?v=1' | asset_url }}">

<!-- JavaScript at bottom except for Modernizr -->
<script src="{{ 'modernizr-2.0.min.js' | asset_url }}"></script>

<!-- For iPhone 4 -->
<link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ 'apple-touch-icon-114x114.png' | asset_url }}">
<!-- For iPad 1-->
<link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ 'apple-touch-icon-72x72.png' | asset_url }}">
<!-- For iPhone 3G, iPod Touch and Android -->
<link rel="apple-touch-icon-precomposed" href="{{ 'apple-touch-icon-precomposed.png' | asset_url }}">
<!-- For Nokia -->
<link rel="shortcut icon" href="{{ 'apple-touch-icon.png' | asset_url }}">
<!-- For everything else -->
<link rel="shortcut icon" href="{{ 'favicon.ico' | asset_url }}">

<!-- http://t.co/y1jPVnT -->
<link rel="canonical" href="/">

{{ content_for_header }}

</head>

<body class="clearfix">
  <div id="container" class="clearfix">
	  <header role="banner" class="clearfix">
		<h1><a href="/"><span class="ir">{{ shop.title }}</span></a></h1>
		<nav>
			<ul class="clearfix">
				<li><a href="/pages/about">About Us</a></li>
				<li><a href="/collections/shop">Shop</a></li>
				<li><a href="http://pixiedusk.tumblr.com/">Blog</a></li>
				<li><a href="/pages/faq#main" rel="highslide-ajax">Help</a></li>
			</ul>
		</nav>
		<aside>
			<p>{{ settings.top_right_shout_out }}</p>
			<ul>
				<li><a href="/cart">My cart ({{ cart.item_count }})</a></li> 
			</ul>
		</aside>
	   </header>

	<div class="content clearfix">

	<div role="main" id="main">
		{{ content_for_layout }}
	</div>

	<div role="complementary">
	</div>

	</div>

	<footer role="contentinfo" class="clearfix">
		<ul>
			<li><a href="/pages/contact-us#main" rel="highslide-ajax">Contact Us</a></li>
			<li><a href="/pages/terms-of-use#main" rel="highslide-ajax">Terms of Use</a></li>
			<li><a href="/pages/privacy-policy#main" rel="highslide-ajax">Privacy Policy</a></li>
			<li><a href="/pages/newsletter-signup#main" rel="highslide-ajax">Sign Up for Newsletter</a></li>
		</ul>
		<ul class="social">
			<li><a href="http://www.facebook.com/pixiedusk" class="facebook"><span class="ir">facebook</span></a></li>
			<li><a href="http://twitter.com/pixieduskshop" class="twitter"><span class="ir">twitter</span></a></li>
		</ul>
	</footer>
  </div>

<script src="//ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.js"></script>
<script>window.jQuery || document.write('<script src="{{ 'jquery-1.6.1.min.js' | asset_url }}">\x3C/script>')</script>

<!-- Scripts -->
<script src="{{ 'plugins.js' | asset_url }}"></script>
<script src="{{ 'script.js' | asset_url }}"></script>

