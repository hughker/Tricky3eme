<!--[if lt IE 7 ]><html class="no-js ie6" lang="en" itemscope itemtype="http://schema.org/{% case template %}{% when 'product' %}Product{% when 'blog' %}Blog{% else %}LocalBusiness{% endcase %}"><![endif]-->
<!--[if IE 7 ]><html class="no-js ie7" lang="en" itemscope itemtype="http://schema.org/{% case template %}{% when 'product' %}Product{% when 'blog' %}Blog{% else %}LocalBusiness{% endcase %}"><![endif]-->
<!--[if IE 8 ]><html class="no-js ie8" lang="en" itemscope itemtype="http://schema.org/{% case template %}{% when 'product' %}Product{% when 'blog' %}Blog{% else %}LocalBusiness{% endcase %}"><![endif]-->
<!--[if (gte IE 9)|(gt IEMobile 7)|!(IEMobile)|!(IE)]><!-->
<html class="no-js" lang="en" itemscope itemtype="http://schema.org/{% case template %}{% when 'product' %}Product{% when 'blog' %}Blog{% else %}LocalBusiness{% endcase %}">
<!--<![endif]-->

<head>
{% include 'meta' %}

<link rel="stylesheet" href="{{ 'mangled.css' | asset_url }}">

<script src="{{ 'modernizr.js' | asset_url }}"></script>

{{ content_for_header }}
{% if template contains 'customers' %}
  {{ "shopify_common.js" | shopify_asset_url | script_tag }}
  {{ "customer_area.js"  | shopify_asset_url | script_tag }}
  {{ 'customer.css' | asset_url | stylesheet_tag }}
{% endif %}
</head>

<body id="{{ template }}" class="clearfix">
 <header role="banner" class="clearfix">
  <h1><a href="/"><span class="ir">{{ shop.title }}</span></a></h1>
 </header>
 <div role="main" class="content clearfix">
   {{ content_for_layout }}
 </div>
 <footer role="contentinfo" class="clearfix">
  <ul id="social">
   <li><a href="http://www.facebook.com/" class="facebook"><span class="ir">facebook</span></a></li>
   <li><a href="http://www.twitter.com/" class="twitter"><span class="ir">twitter</span></a></li>
  </ul>
 </footer>

<script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.0/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="{{ 'jquery-1.7.0.min.js' | asset_url }}">\x3C/script>')</script>

<!-- Scripts -->
<script defer src="{{ 'plugins.js' | asset_url }}"></script>
<script defer src="{{ 'script.js' | asset_url }}"></script>

<!--[if (lt IE 9) & (!IEMobile)]>
<script src="{{ 'DOMAssistantCompressed-2.8.js' | asset_url }}"></script>
<script src="{{ 'selectivizr-1.0.2.min.js' | asset_url }}"></script>
<script src="{{ 'respond.proxy.js' | asset_url }}"></script>
<![endif]-->

